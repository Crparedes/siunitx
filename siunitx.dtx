\newcommand*{\si@symbol@redefine}{%
  \si@bool@if@T\si@symbol@redefine@bool{%
    \@ifpackageloaded{textcomp}{%
      \sisetup{
        symbols/maths/degree = \text{\textdegree},
        symbols/text/degree  = \textdegree
      }%
      \@ifpackageloaded{mathptmx}{}{%
