\newcommand*{\si@unit@parse@store@qual}[1]{%
  \si@unit@parse@last@if@T{unit}{%
    \si@unit@parse@store@add@o{\si@unit@parse@qual@format{#1}}%
    \si@unit@parse@store@type{qual}%
