  \si@bool@if@F\si@num@post@zero@exp@bool{%
    \si@expand@tlist\si@tempa{%
      \si@num@in@exp@int
      \si@num@in@exp@dec
      \si@num@in@exp@comp@int
      \si@num@in@exp@comp@dec
    }%
    \si@in@if@T\si@tempa{0}{%
      \def\si@tempb##10000##2\si@stop{%
