\input{regression-test}

\RequirePackage{siunitx}

\START
\AUTHOR{Joseph Wright}

\ExplSyntaxOn

\OMIT
\cs_set_protected:Npn \test:n #1
  {
    \siunitx_number_format:nN {#1} \l_tmpa_tl
    \tl_show:N \l_tmpa_tl
  }
\TIMO

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

\TEST { Basic~formatting:~integers }
  {
    \test:n { 1 }
    \test:n { 123 }
    \test:n { 123456789 }
    \test:n { 12345678901234567890 }
    \test:n { 00001 }
    \test:n { 00000 }
  }

\END