\input{regression-test}

\RequirePackage[enable-debug,check-declarations]{expl3}

\ExplSyntaxOn
\debug_on:n { deprecation }
\ExplSyntaxOff

\RequirePackage{siunitx}

\START

\ExplSyntaxOn

\OMIT
\cs_set_protected:Npn \test:n #1
  {
    \siunitx_number_format:nN {#1} \l_tmpa_tl
    \tl_show:N \l_tmpa_tl
  }
\TIMO

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

\TEST { Basic~formatting:~integers }
  {
    \test:n { 1 }
    \test:n { 123 }
    \test:n { 123456789 }
    \test:n { 12345678901234567890 }
    \test:n { 00001 }
    \test:n { 00000 }
  }

\TEST { Basic~formatting:~reals }
  {
    \test:n { 1.2 }
    \test:n { 1234.5678 }
    \test:n { .1234 }
    \test:n { 12345. }
  }

\TEST { Basic~formatting:~exponents }
  {
    \test:n { e10 }
    \test:n { e0 }
    \test:n { 1e2 }
    \test:n { 12345e06 }
    \test:n { 12.34e5 }
  }

\TEST { Basic~formatting:~signs }
  {
    \test:n { +e10 }
    \test:n { -1 }
    \test:n { +-10.234 }
    \test:n { 12.34e-5 }
  }

\TEST { Basic~formatting:~uncertainties }
  {
    \test:n { 123(4) }
    \test:n { 123.456(7) }
    \test:n { 123.456(07) }
    \test:n { 123.456(70) }
    \test:n { 123.4(56) }
    \test:n { 123.4(00) }
    \test:n { 123 +- 4 }
    \test:n { 123.456 +- 000.007 }
    \test:n { 123.456 +- 0.007 }
    \test:n { 123.456 +- 0.070 }
    \test:n { 123.4 +- 5.6 }
    \test:n { 123.4 +- 5.678 }
    \test:n { 123.4 +- 0.06 }
  }

\TEST { Basic~formatting:~comparators }
  {
    \test:n { > 1.2 }
    \test:n { \le + 1234.5678 }
    \test:n { >> 5 }
  }

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

\TEST { Parsing~number }
  {
    \siunitx_number_parse:nN { 123.456 +- 0.070 } \l_tmpa_tl
    \tl_show:N \l_tmpa_tl
  }

\OMIT
\tl_set:Nn \l_tmpa_tl { { } + { 123 } { 456 } { } + { 7 } }
\tl_set:Nn \l_tmpb_tl { { < } - { 0 } { 12345 } { { S } { 6 } } - { 1 } }
\TIMO

\TESTEXP { Formatting~parsed~numbers }
  {
    \siunitx_number_format:N \l_tmpa_tl \NEWLINE
    \siunitx_number_format:N \l_tmpb_tl \NEWLINE
  }

\OMIT
\cs_set_protected:Npn \test:n #1
  {
    \siunitx_number_parse:nN {#1} \l_tmpa_tl
    \tl_set:Nx \l_tmpa_tl
      { \siunitx_number_format:NN \l_tmpa_tl & }
    \tl_show:N \l_tmpa_tl
  }
\TIMO

\TEST { Formatting~for~tabular~alignment }
  {
    \test:n { >10 }
    \test:n { 123 }
    \test:n { 123.456 }
    \test:n { 123.456(789) }
    \group_begin:
      \keys_set:nn { siunitx } { separate-uncertainty = true }
      \test:n { 123.456 \pm 0.789 }
    \group_end:
    \test:n { 123e10 }
    \test:n { +1 }
    \test:n { e11 }
    \test:n { -e11 }
  }

